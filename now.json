{
  "version": 2,
  "name": "petgram-curso-platzi-react-avanzado",
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).json",
      "dest": "/$1.json"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}
